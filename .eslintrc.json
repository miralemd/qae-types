{
  "root": true,
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 10
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "max-len": 0,
    "no-plusplus": 0,
    "no-bitwise": 0,
    "prettier/prettier": 2,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
